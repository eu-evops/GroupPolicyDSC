[ClassVersion("1.0.0.0"), FriendlyName("GroupPolicyAssignment")]
class MSFT_GroupPolicyAssignment : OMI_BaseResource
{
    [Required, Description("Name of the GPO policy to create")] String Name;
    [Required, Description("OU to assign GPO to")] String OUPath;
    [Description("Flag to indicate whether the policy is enforced")] Boolean Enforced;
    [Description("Flag to indicate whether the policy is linked")] Boolean Linked = true;
};
